# ALTER EGO PWA Environment Configuration
# Copy this file to .env.local and customize your settings

# Development settings
NODE_ENV=development
REACT_APP_VERSION=1.0.0

# Build optimization
GENERATE_SOURCEMAP=true
BUILD_PATH=dist

# API Configuration
REACT_APP_OPENAI_API_BASE=https://api.openai.com/v1
REACT_APP_ELEVENLABS_API_BASE=https://api.elevenlabs.io/v1

# Backend Configuration (for open-source models)
REACT_APP_BACKEND_URL=http://127.0.0.1:8000

# Security Configuration
REACT_APP_ENABLE_DEV_TOOLS_BLOCKING=false
REACT_APP_ENABLE_ADVANCED_SECURITY=false

# Performance Monitoring
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_METRICS_COLLECTION=true
REACT_APP_LOG_LEVEL=info

# GitHub Pages Deployment
GITHUB_PAGES=false
PUBLIC_URL=/

# Feature Flags
REACT_APP_ENABLE_OPEN_SOURCE=true
REACT_APP_ENABLE_VOICE_RECOGNITION=false

# Bundle Analysis
ANALYZE_BUNDLE=false
